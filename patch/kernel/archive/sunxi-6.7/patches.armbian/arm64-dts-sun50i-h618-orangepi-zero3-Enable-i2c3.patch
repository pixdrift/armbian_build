From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: pixdrift <devnull@pixeldrift.net>
Date: Thu, 4 Jan 2024 12:19:18 +0000
Subject: Enable i2c3 configuration for zero3

Signed-off-by: pixdrift <devnull@pixeldrift.net>
---
 arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts | 6 ++++++
 1 file changed, 6 insertions(+)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts b/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts
index c95eddbd5d35..81b654804279 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h618-orangepi-zero3.dts
@@ -95,10 +95,16 @@ reg_dcdc3: dcdc3 {
 			};
 		};
 	};
 };

+&i2c3 {
+        pinctrl-names = "default";
+        pinctrl-0 = <&i2c3_ph_pins>;
+        status = "okay";
+};
+
 &pio {
 	vcc-pc-supply = <&reg_dldo1>;
 	vcc-pf-supply = <&reg_dldo1>;
 	vcc-pg-supply = <&reg_aldo1>;
 	vcc-ph-supply = <&reg_dldo1>;
